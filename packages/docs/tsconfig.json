{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "dist",
        "paths": {
            "@/*": [
                "./*"
            ]
        }
    },
    "vueCompilerOptions": {
        "vitePressExtensions": [".md"],
    },
    "include": [
        "./.vitepress/**/*",
        "./**/*.ts",
        "./**/*.d.ts",
        "./**/*.tsx",
        "./**/*.vue"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
