{
    "name": "@michaelcozzolino/flyonui-vue",
    "version": "0.0.0",
    "author": "Michael Cozzolino",
    "license": "MIT",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/michaelcozzolino/flyonui-vue.git"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/flyonui-vue.mjs",
            "require": "./dist/flyonui-vue.umd.js"
        },
        "./dist/style.css": {
            "import": "./dist/style.css",
            "require": "./dist/style.css"
        },
        "./*": "./dist/*"
    },
    "main": "./dist/flyonui-vue.umd.js",
    "module": "./dist/flyonui-vue.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=18.x",
        "npm": ">=10.x"
    },
    "scripts": {
        "dev": "vitepress dev docs",
        "start": "vitepress serve docs",
        "build": "vitepress build docs && cp -r ./docs/assets/* ./docs/.vitepress/dist/assets/",
        "build:package": "vite build",
        "build:types": "vue-tsc --declaration --emitDeclarationOnly && tsc-alias -p tsconfig.json",
        "build:production": "yarn build:package && yarn build:types",
        "lint": "eslint . --fix",
        "test": "vitest run",
        "test-coverage": "vitest run --coverage",
        "type-check": "vue-tsc --noEmit -p tsconfig.json",
        "publish": "yarn publish --access public"
    },
    "peerDependencies": {
        "tailwindcss": "^3",
        "vue": "^3"
    },
    "dependencies": {
        "@vueuse/components": "^12.3.0",
        "@vueuse/core": "^12.3.0",
        "autoprefixer": "^10.4.20",
        "classnames": "2.5.1",
        "flyonui": "^1.3.0",
        "highlight.js": "^11.11.1",
        "nanoid": "5.0.9",
        "tailwindcss": "^3",
        "vue-code-highlighter": "^1.1.2",
        "vue-router": "^4.4.5"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^3.12.1",
        "@eslint/compat": "^1.2.4",
        "@iconify/vue": "^4.2.0",
        "@rushstack/eslint-patch": "1.10.3",
        "@types/node": "20.12.12",
        "@vitejs/plugin-vue": "5.0.4",
        "@vitest/coverage-v8": "1.6.0",
        "@vitest/eslint-plugin": "^1.1.7",
        "@vue/eslint-config-typescript": "^14.1.3",
        "@vue/test-utils": "2.4.6",
        "@vue/tsconfig": "0.5.1",
        "eslint": "8.57.0",
        "eslint-config-standard": "17.1.0",
        "eslint-plugin-align-import": "^1.0.0",
        "eslint-plugin-vue": "9.26.0",
        "eslint-plugin-vue-scoped-css": "2.8.0",
        "jsdom": "24.1.0",
        "postcss": "8.4.38",
        "sass-embedded": "^1.83.0",
        "tsc-alias": "1.8.10",
        "typescript": "5.4.5",
        "vite": "^5.4.10",
        "vitepress": "1.5.0",
        "vitest": "1.6.0",
        "vue-tsc": "2.0.19"
    }
}
